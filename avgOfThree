import java.util.Scanner;

public class avgOfThree
{
    public static void main(String[] args)
    {
        int a;
        int b;
        int c;
        Scanner sc = new Scanner(System.in);
        a = sc.nextInt();
        b = sc.nextInt();
        c = sc.nextInt();
        System.out.println("\n" + (double)(a+b+c)/3);
    }
}
